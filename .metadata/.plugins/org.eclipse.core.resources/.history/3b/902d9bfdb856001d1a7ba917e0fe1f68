package executavel;

public class ArrayMatriz {

	public static void main(String[] args) {
		
		int notas[][] = new int[3][3];
		
		//notas[0][0] = 80;
		//notas[0][1] = 70;
		//notas[0][2] = 80;
		
		
		for (int i = 0; i < notas.length; i++) {
			for(int j = 0; j < notas.length; j++) {
				notas[i][j] = 5;
				System.out.println(notas[i][j]);
			}
		}

	}

}
